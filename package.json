    "react-feather": "^2.0.9",
    "react-intl": "^5.13.3",
    "react-suspense-loadable": "^1.0.0",
    "react-typography": "^0.16.19",
    "resize-observer-polyfill": "^1.5.1",
    "typography": "^0.16.19",
    "use-media": "^1.4.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.0.1",
    "@linaria/stylelint": "^3.0.0-beta.1",
    "@types/node": "^14.14.35",
