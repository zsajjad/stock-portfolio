    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern server",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern server --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^4.0.12",
    "@formatjs/intl-relativetimeformat": "^8.1.3",
